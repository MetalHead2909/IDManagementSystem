    <div class="container-fluid">
        <div class="form-container">

            <div class="header">
                <h1>Sign Up!</h1>
            </div>

            <form action="/app1.js" method="post">

                <div class="name row">
                    <div class="col-lg-4 col-md-4">
                        <label for="fname">First name:</label>
                        <input style="background-color: rgba(0, 0, 0, 0.2);" type="text" id="fname" name="fname">
                    </div>
                    <div class="col-lg-4 col-md-4">
                        <label for="midname">Middle name:</label>
                        <input style="background-color: rgba(0, 0, 0, 0.2);" type="text" id="midname" name="midname">
                    </div>
                    <div class="col-lg-4 col-md-4">
                        <label for="lname">Last name:</label>
                        <input style="background-color: rgba(0, 0, 0, 0.2);" type="text" id="lname" name="lname">
                    </div>
                    <br>
                </div>

                <div class="Gender">
                    <label for="gender">Select your gender</label>
                    <select style="background-color: rgba(0, 0, 0, 0.2);" name="gender">
                        <option value="Male">Male</option>
                        <option value="Female">Female</option>
                        <option value="Other">other</option>
                    </select>
                    <br>
                </div>

                <div class="address">
                    <label>Address:</label>
                    <input style="background-color: rgba(0, 0, 0, 0.2);" type="text" id="street" name="street" placeholder="Street">
                    <br>
                    <div class="address-2">
                        <input style="background-color: rgba(0, 0, 0, 0.2);" type="text" id="city" name="city" placeholder="City">
                        <input style="background-color: rgba(0, 0, 0, 0.2);" type="text" id="state" name="state" placeholder="State">
                    </div>

                    <div class="address-3">
                        <input style="background-color: rgba(0, 0, 0, 0.2);" type="text" id="pincode" name="pincode" placeholder="Pincode">
                        <input style="background-color: rgba(0, 0, 0, 0.2);" type="text" id="country" name="country" placeholder="Country">
                    </div>
                    <br>
                </div>

                <div class="email-container">
                    <label for="exampleInputEmail1" class="form-label">Email address</label>
                    <input style="background-color: rgba(0, 0, 0, 0.2);" type="email" class="form-control input-box" id="exampleInputEmail1"
                        placeholder="eg. abcd1234@gmail.com" name="email">
                    <br>
                </div>

                <div class="password-container">
                    <label for="exampleInputPassword1" class="form-label">Password</label>
                    <input style="background-color: rgba(0, 0, 0, 0.2);" type="password" class="form-control input-box" id="exampleInputPassword1"
                        placeholder="* * * * * * * *" name="password">
                    <br>
                </div>

                <div class="dob-container">
                    <label for="dob">Date of Birth:</label>
                    <input style="background-color: rgba(0, 0, 0, 0.2);" type="date" id="dob" name="dob">
                </div>

                <button type="submit" class="btn btn-primary" style="margin-bottom: 20px;">Register</button>
            </form>
        </div>
    </div>
</body>

